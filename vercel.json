{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "vercel-python@0.5.0"
    }
  },
  "builds": [
    {
      "src": "api/generate_spreadsheet.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate_spreadsheet",
      "dest": "/api/generate_spreadsheet.py"
    }
  ]
}
